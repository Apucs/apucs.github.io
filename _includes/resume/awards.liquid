<div class="list-groups">
  {% for content in data[1] %}
    <div class="list-group col-md-6">
      <table class="table-cv list-group-table">
        <tbody>
          <tr>
            {% if content.icon %}
              <td class="list-group-category-icon">
                <i class="{{ content.icon }}"></i>
              </td>
            {% else %}
              <td class="list-group-category-icon"></td>
            {% endif %}
            <td colspan="2" class="list-group-category">
              {{ content.title }}{% if content.date %} ({{ content.date | split: '-' | first }}){% endif %}
            </td>
          </tr>
          <tr>
            <td></td>
            <td class="list-group-name">
              <b>{% if content.awarder_url %}<a href="{{ content.awarder_url }}" target="_blank">{{ content.awarder }}</a>{% else %}{{ content.awarder }}{% endif %}</b>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  {% endfor %}
</div>
